<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <FrameLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="48dp"
                android:background="#3c3c42"
                android:gravity="center"
                android:text="@string/pi_basedata_title"
                android:textColor="#FFFFFF"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/back_gt"
                android:layout_width="76dp"
                android:layout_height="40dp"
                android:layout_gravity="left|center"
                android:layout_marginLeft="10dp"
                android:background="@drawable/a_gt_inparam_top"
                android:gravity="center"
                android:text="@string/back"
                android:textColor="#9398a6"
                android:textSize="17sp" />
        </FrameLayout>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"
            android:text="@string/AUT_under_test"
            android:textColor="#FFFFFF"
            android:textSize="15sp" />


        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true">

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="60dp"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="15dp">

                <TextView
                    android:id="@+id/selected_app_bg"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:background="@drawable/seekbar_border" />

                <TextView
                    android:id="@+id/app_pic"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="8dp"
                    android:background="@drawable/pi_procinfo"
                    android:gravity="left" />

                <TextView
                    android:id="@+id/selected_apn"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_toRightOf="@+id/app_pic"
                    android:gravity="center"
                    android:singleLine="true"
                    android:text="@string/AUT_choose"
                    android:textColor="#b7bdcf"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/select_tested_pkn"
                    android:layout_width="wrap_content"
                    android:layout_height="15dp"
                    android:layout_alignBottom="@+id/app_pic"
                    android:layout_alignLeft="@+id/selected_apn"
                    android:layout_marginBottom="5dp"
                    android:gravity="top"
                    android:singleLine="true"
                    android:text="@string/na"
                    android:textColor="#b7bdcf"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/select_app"
                    android:layout_width="35dip"
                    android:layout_height="50dip"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentRight="true"
                    android:background="@android:color/transparent"
                    android:gravity="right" />

                <TextView
                    android:id="@+id/select_app_icon"
                    android:layout_width="20dip"
                    android:layout_height="20dip"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:layout_marginRight="8dp"
                    android:background="@drawable/a_expand_r_big"
                    android:gravity="right" />
            </RelativeLayout>
        </RelativeLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp">

            <TextView
                android:id="@+id/button_start"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="10dp"
                android:layout_weight="1"
                android:background="@drawable/switch_on_border"
                android:gravity="center"
                android:text="@string/start"
                android:textColor="#FFFFFF"
                android:textSize="16sp" />


            <TextView
                android:id="@+id/debug_clear_tv"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_weight="1"
                android:background="@drawable/switch_on_border"
                android:gravity="center"
                android:text="@string/clear"
                android:textColor="#FFFFFF"
                android:textSize="16sp" />


            <TextView
                android:id="@+id/button_pulldata"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:background="@drawable/switch_on_border"
                android:gravity="center"
                android:text="导出数据"
                android:textColor="#FFFFFF"
                android:textSize="16sp" />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="4dp"
                android:text="@string/pi_basedata_float"
                android:textColor="#b7bdcf"
                android:textSize="15sp" />

            <RelativeLayout
                android:id="@+id/basedata_switch"
                android:layout_width="65dp"
                android:layout_height="25dp"
                android:layout_alignParentRight="true"
                android:layout_gravity="center"
                android:background="@drawable/seekbar_border"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_float_on"
                    android:layout_width="25dp"
                    android:layout_height="20dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="3dp"
                    android:background="@drawable/swbtn_selected"
                    android:gravity="center"
                    android:textColor="#FFFFFF"
                    android:textSize="15sp" />

                <TextView
                    android:id="@+id/tv_float_off"
                    android:layout_width="25dp"
                    android:layout_height="20dp"
                    android:layout_marginTop="3dp"
                    android:layout_toRightOf="@id/tv_float_on"
                    android:background="@drawable/swbtn_default"
                    android:gravity="center"
                    android:text="@string/off"
                    android:textColor="#FFFFFF"
                    android:textSize="15sp" />
            </RelativeLayout>
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10dp"
            android:paddingTop="15dp"
            android:text="@string/pi_basedata_switch"
            android:textColor="#b7bdcf"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/page_switching_tv"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="8dp"
            android:paddingLeft="10dp"
            android:text="页面切换总次数：0次，>500ms：0次"
            android:textColor="#FFFFFF" />

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="20dp"
            android:text="@string/pi_basedata_framerate"
            android:textColor="#b7bdcf"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/frame_rate_tv"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="8dp"
            android:paddingLeft="10dp"
            android:text="10帧0次，20帧0次"
            android:textColor="#FFFFFF" />

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="20dp"
            android:text="@string/pi_basedata_sm"
            android:textColor="#b7bdcf"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/basedata_sm_tv"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="8dp"
            android:paddingLeft="10dp"
            android:textColor="#FFFFFF" />


        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="20dp"
            android:text="@string/pi_basedata_bgcpu"
            android:textColor="#b7bdcf"
            android:textSize="18sp" />


        <TextView
            android:id="@+id/bg_cpu_tv"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="8dp"
            android:paddingLeft="10dp"
            android:text="应用后台时间:0小时，cpu使用时长：0分钟"
            android:textColor="#FFFFFF" />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10dp"
            android:paddingTop="15dp"
            android:text="@string/desc"
            android:textColor="#b7bdcf"
            android:textSize="18sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingTop="10dp"
            android:text="@string/basedata_desc"
            android:textColor="@drawable/text_title_gray"
            android:textSize="14sp" />


        <TextView
            android:id="@+id/debug_data_tv"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="15dp"
            android:background="@drawable/switch_on_border"
            android:gravity="center"
            android:text="导出"
            android:textColor="#FFFFFF"
            android:textSize="18sp"
            android:visibility="gone" />


    </LinearLayout>
</ScrollView>